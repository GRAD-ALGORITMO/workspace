algoritmo "semnome"

   procedimento imprimir()
   inicio
      escreval("O funcionário ",nome," recebeu R$",somaSalarial," em ",anoRecebimento)
   fimProcedimento

var
   nome: caracter
   somaSalarial: real
   anoRecebimento: inteiro
   
inicio
   nome <- "elberth"
   somaSalarial <- 10000
   anoRecebimento <- 2021

   imprimir()
fimalgoritmo

